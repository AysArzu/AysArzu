 System.out.println("Which number is bigger");

        int sayi1 = 43;
        int sayi2 = 36;
        int sayi3 = 54;
        int bigNumber = sayi1;
        if (sayi1 < sayi2)
            bigNumber = sayi2;
        if (sayi2 < sayi3)
            bigNumber = sayi3;
        {
            System.out.println("Big number is:" + bigNumber);
        }
