
public class Odevler {
    public static void main(String[] args) {

      int sayi=29;
      boolean isPrime=true;

        for (int i = 2; i <= sayi-1; i++) {
            if(sayi%i==0){
              isPrime=false;}

        } if(isPrime){
        System.out.println("Asal sayi");}
else{
            System.out.println("Asal sayı değildir");
    }}}
