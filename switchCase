
    char grade='C';

       switch (grade) {
           case 'A':
               System.out.println("Mükemmel, gectiniz");
               break;
           case 'B':
               System.out.println("Çok güzel, gectiniz");
           break;
           case 'C':
               System.out.println("İyi, gectiniz");
               break;
           case 'D':
               System.out.println("Fena değil, gectiniz");
               break;
           case 'F':
               System.out.println("Mükemmel, gectiniz");
               break;
        default:{
               System.out.println("Kaldınız");
           }
       }
